<section class="widget">
  <header>
    <h4><i class="eicon-export"></i> Exports </h4>
  </header>
  <div class="body">
      <table class="table table-hover">
        <thead>
            <tr>
                <th> Created at </th>
                <th> Status </th>
                <th> Download </th>
            </tr>
        </thead>
        <tbody>
            <% @exports.each do |export| %>
                <tr>
                    <td><% export.created_at %></td>
                    <td><% export.status %></td>
                    <td></td>
                </tr>
            <% end %>
        </tbody>
      </table>
  </div>      
</section>
