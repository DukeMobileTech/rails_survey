
<table class="table">
  <tr>
    <th> Name </th>
    <th> Date Created </th>
    <th> Date Published </th>
    <th> # of questions </th>
    <th> # of participants </th>
    <th> Daily frequency </th>
  </tr>

<% @instruments.each do |instrument| %>
    <tr>
        <td> <%= link_to instrument.title, stat_path(:id => instrument.id, :title => instrument.title) %>  </td>
        <td> <%= instrument.created_at %> </td>
        <td> date </td>
        <td> <%= instrument.questions.count %></td>
        <td> count 'em </td>
        <td> TBD </td>
    </tr>
<% end %>

</table>
