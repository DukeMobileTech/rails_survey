<h1><%= @instrument.title %></h1>
<section class="widget">
  <p>Choose project to copy instrument, <i><%= @instrument.title %></i>, to:</p>
  <%= form_for @instrument, url: update_copy_project_instrument_path(current_project, @instrument) do |f| %>
      <% @projects.each do |project| %>
          <% unless project == current_project %>
              <%= radio_button_tag(:end_project, project.id) %>
              <%= label_tag(:end_project, project.name) %> </br>
          <% end %>
      <% end %>
      <%= f.submit 'Copy Instrument', class: 'btn btn-primary btn-large' %>
  <% end %>
</section>