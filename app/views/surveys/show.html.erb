<h1>Survey for <%= @survey.instrument.title %></h1>

<% @survey.responses.each do |response| %>
  <p>
    <%= response.question.question_identifier %><br />
    <%= response.question.text %><br />
    <ol start="0">
    <% response.question.options.each do |option| %>
      <li><%= option.text %></li>
    <% end %>
    </ol>
    <%= response.text %><br />
    <% if response.other_response %>
      <%= response.other_response %><br />
    <% end %>
    <%= response.created_at %>
  </p>
<% end %>
